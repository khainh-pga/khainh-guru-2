{
  "name": "khainh-guru-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "export NODE_ENV=development && export SLS_STAGE=dev && npm run deploy:env",
    "deploy:env": "sls deploy --stage $SLS_STAGE",
    "deploy:function": "sls deploy function --stage $SLS_STAGE",
    "deploy:production": "export NODE_ENV=production && export SLS_STAGE=prod && npm run deploy:env",
    "remove:env": "sls remove --stage $SLS_STAGE",
    "package": "sls package",
    "lint": "tslint 'src/**/*.ts{,x}'",
    "start": "serverless offline start",
    "start:user": "cd src/stacks/user && serverless offline start",
    "test": "jest ./src",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.202.0",
    "@aws-sdk/lib-dynamodb": "^3.202.0",
    "aws-sdk": "^2.1245.0",
    "dotenv": "^16.0.3",
    "lambda-utilities": "^0.2.0",
    "lamprox": "^0.15.2",
    "omit-empty-es": "^1.1.3",
    "prettier": "^2.7.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@shelf/jest-dynamodb": "^3.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "serverless": "^3.23.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^11.2.3",
    "serverless-plugin-split-stacks": "^1.11.3",
    "serverless-webpack": "^5.10.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "@babel/core": "^7.19.6"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn lint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
